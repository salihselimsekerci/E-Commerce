@using E_Commerce.Identity
@model IEnumerable<ApplicationUser>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Kullanıcı Listesi</h3>
<table class="table table-bordered">
    <tr>
        <th>Adı</th>
        <th>Soyadı</th>
        <th>Kullanıcı Adı</th>


    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Ad</td>
            <td>@item.Soyad</td>
            <td>
                @item.UserName
            </td>
           


            <td><a class="btn btn-primary" href="@Url.Action("Details",new { id=item.Id})"><i class="fa fa-fw fa-edit"></i>Sipariş Detay</a> </td>

        </tr>
    }
    <tbody>
        <tr>
            <th>Adı</th>
            <th>Soyadı</th>
            <th>Kullanıcı Adı</th>
        </tr>
    </tbody>
</table>


